<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
<session-config>
        <session-timeout>60</session-timeout>
</session-config>
	<display-name>hypothesis.browser</display-name>
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>hypothesis.browser-0.0.1</web-resource-name>
			<url-pattern>/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>hypothesis.browser-0.0.1</role-name>
		</auth-constraint>
	</security-constraint>
	<login-config>
		<auth-method>BASIC</auth-method>
		<realm-name>hypothesis.browser-0.0.1</realm-name>
	</login-config>
	<security-role>
		<role-name>hypothesis.browser-0.0.1</role-name>
	</security-role>
</web-app>